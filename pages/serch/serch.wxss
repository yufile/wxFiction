/* pages/serch/serch.wxss */
.serch {
  width: 750rpx;
  height: 100%;
}
.serch-top {
  width: 750rpx;
  height: 108rpx;
}
.tag-top {
  width: 100%;
  height: 100%;
}
.tag {
  width: 90%;
  margin: 10rpx auto;
  text-align: center;
  display: grid;
  grid-template-columns:repeat(2, 1fr 1fr);
  grid-template-rows:60rpx;
}
.title {
  margin: 20rpx 0 30rpx 20rpx;
  color: rgb(143, 143, 143);
}
.tag-top van-button {
  float: right;
  margin-right: 20rpx;
}
.serch-data {
  margin-top: 100rpx;
}